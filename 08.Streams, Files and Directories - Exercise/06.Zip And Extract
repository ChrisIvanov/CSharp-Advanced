using System;
using System.IO.Compression;

namespace _06.ZipAndExtract
{
    class Program
    {
        static void Main(string[] args)
        {
            ZipFile.CreateFromDirectory(@"G:\Engeneering and IT\ZipFileDemo1", @"G:\Engeneering and IT\ZipFileDemo2\MyZipFile.zip");
            ZipFile.ExtractToDirectory(@"G:\Engeneering and IT\ZipFileDemo2\MyZipFile.zip", @"G:\Engeneering and IT\ZipFileDemoResult");
        }
    }
}
